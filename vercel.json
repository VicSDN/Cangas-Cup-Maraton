{
  "redirects": [
    {
      "source": "/",
      "destination": "/2025",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*).(avif|webp|svg|png|jpg|gif)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, stale-while-revalidate=120"
        }
      ]
    }
  ]
}
